// Generated by CoffeeScript 1.3.1
(function() {

  angular.module('bootstrap.directives', []).directive("dropdown", [
    function(scope, element, attrs) {
      jQuery('html').on('click', function() {
        return element.removeClass('open');
      });
      return jQuery('.dropdown-toggle', element).on('click', function(e) {
        return element.toggleClass('open');
      });
    }
  ]);

}).call(this);
